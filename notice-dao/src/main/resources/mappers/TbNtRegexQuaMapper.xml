<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.silita.notice.dao.TbNtRegexQuaMapper" >

  <select id="queryNoticeId" resultType="java.lang.String" parameterType="map">
    SELECT
      qua_regex AS quaRegex
    FROM
      tb_nt_regex_qua
    WHERE nt_id = #{id}
  </select>
</mapper>